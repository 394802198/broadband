<mapper namespace="com.tm.broadband.mapper.TMSComboWholesalerMapper">

	<!-- DEFINITION AREA -->

	<sql id="tMSComboWholesalerColumns">
		tmscw.`id`				as tmscw_id,
		tmscw.`name`				as tmscw_name,
		tmscw.`wholesaler_id`				as tmscw_wholesaler_id,
		tmscw.`wholesale_price`				as tmscw_wholesale_price,
		tmscw.`selling_price`				as tmscw_selling_price,
		tmscw.`earned_price`				as tmscw_earned_price,
		tmscw.`is_official_combo`				as tmscw_is_official_combo
	</sql>

	<resultMap type="TMSComboWholesaler" id="tMSComboWholesalerResultMap">
		<id property="id" column="tmscw_id" />
		<result property="name" column="tmscw_name" />
		<result property="wholesaler_id" column="tmscw_wholesaler_id" />
		<result property="wholesale_price" column="tmscw_wholesale_price" />
		<result property="selling_price" column="tmscw_selling_price" />
		<result property="earned_price" column="tmscw_earned_price" />
		<result property="is_official_combo" column="tmscw_is_official_combo" />
	</resultMap>

	<sql id="tMSComboWholesalerWhere">
		<where>
			<if test="params.id != null">tmscw.id = #{params.id}</if>
			<if test="params.name != null">and tmscw.name = #{params.name}</if>
			<if test="params.wholesaler_id != null">and tmscw.wholesaler_id = #{params.wholesaler_id}</if>
			<if test="params.wholesale_price != null">and tmscw.wholesale_price = #{params.wholesale_price}</if>
			<if test="params.selling_price != null">and tmscw.selling_price = #{params.selling_price}</if>
			<if test="params.earned_price != null">and tmscw.earned_price = #{params.earned_price}</if>
			<if test="params.is_official_combo != null">and tmscw.is_official_combo = #{params.is_official_combo}</if>
		</where>
	</sql>

	<!-- // DEFINITION AREA -->

	<!-- =================================================================================== -->

	<!-- SELECT AREA -->

	<select id="selectTMSComboWholesaler" parameterType="TMSComboWholesaler" resultType="TMSComboWholesaler">
		select * from tms_combo_wholesaler as tmscw
		<include refid="tMSComboWholesalerWhere"/>
	</select>

	<!-- select tMSComboWholesaler page -->
	<select id="selectTMSComboWholesalersByPage" parameterType="Page" resultMap="tMSComboWholesalerResultMap">
		select 
			<include refid="tMSComboWholesalerColumns"/>
			from tms_combo_wholesaler as tmscw
			<include refid="tMSComboWholesalerWhere"/>
			<if test="params.orderby != null">${params.orderby}</if>
			limit #{pageOffset}, #{pageSize}
	</select>

	<!-- select tMSComboWholesaler amount -->
	<select id="selectTMSComboWholesalersSum" parameterType="Page" resultType="int">
		select count(*) from tms_combo_wholesaler as tmscw
		<include refid="tMSComboWholesalerWhere"/>
	</select>

	<!-- // END SELECT AREA -->

	<!-- =================================================================================== -->

	<!-- INSERT AREA -->

	<insert id="insertTMSComboWholesaler" parameterType="TMSComboWholesaler" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO `tms_combo_wholesaler`(
			`name`, `wholesaler_id`, `wholesale_price`, `selling_price`, 
			`earned_price`, `is_official_combo`
		) VALUES (
			#{name}, #{wholesaler_id}, #{wholesale_price}, #{selling_price}, 
			#{earned_price}, #{is_official_combo}
		)
	</insert>

	<!-- // END INSERT AREA -->

	<!-- =================================================================================== -->

	<!-- UPDATE AREA -->

	<!-- update tMSComboWholesaler -->
	<update id="updateTMSComboWholesaler" parameterType="TMSComboWholesaler">
		update tms_combo_wholesaler
		<set>
			<if test="name != null">name = #{name},</if>
			<if test="wholesaler_id != null">wholesaler_id = #{wholesaler_id},</if>
			<if test="wholesale_price != null">wholesale_price = #{wholesale_price},</if>
			<if test="selling_price != null">selling_price = #{selling_price},</if>
			<if test="earned_price != null">earned_price = #{earned_price},</if>
			<if test="is_official_combo != null">is_official_combo = #{is_official_combo}</if>
		</set>
		<where>
			<if test="params.id != null">id = #{params.id}</if>
		</where>
	</update>

	<!-- // END UPDATE AREA -->

	<!-- =================================================================================== -->
	<!-- DELETE AREA -->

	<delete id="deleteTMSComboWholesalerById" parameterType="int">
		DELETE FROM tms_combo_wholesaler WHERE id = #{0}
	</delete>

	<!-- // END DELETE AREA -->

</mapper>